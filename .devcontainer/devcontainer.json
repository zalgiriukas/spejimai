
{
  "name": "Zalgiriukas (Netlify Functions + Blobs)",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "forwardPorts": [8888],
  "customizations": {
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint",
        "streetsidesoftware.code-spell-checker"
      ]
    }
  },
  "postCreateCommand": "npm install && npm install -g netlify-cli",
  "postStartCommand": "echo 'Ready. Run: netlify dev' && netlify --version",
  "remoteUser": "node"
}
